pkgname=python-aiowinreg
_pkgname=aiowinreg
pkgver=0.0.13
pkgrel=1
pkgdesc='Windows registry reader.'
arch=('any')
url='https://pypi.org/project/aiowinreg/#files'
license=('MIT')
depends=('python')
makedepends=('python-setuptools')
source=("https://github.com/skelsec/aiowinreg/archive/refs/tags/$pkgver.tar.gz")
sha512sums=('dd482b6e35ec8dd433870e71808c2b1aa602dac2d870dc4ce83d3828ab7333ac113190b7e67748d168db0367666973f627405e4ca337b03bbde982c77dfd41d6')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --prefix=/usr --root="$pkgdir" -O1 --skip-build
}

