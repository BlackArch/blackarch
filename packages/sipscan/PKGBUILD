pkgname='sipscan'
pkgver='0.1'
pkgrel=4
epoch=1
pkgdesc='A sip scanner.'
url='http://www.hackingvoip.com/sec_tools.html'
groups=('blackarch' 'blackarch-windows' 'blackarch-scanner' 'blackarch-voip')
license=('custom')
arch=('any')
source=('http://www.hackingvoip.com/tools/sipscan.msi')
