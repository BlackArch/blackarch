pkgname=skype-dump
pkgver=0.1
pkgrel=8
groups=('blackarch' 'blackarch-windows' 'blackarch-cracker')
pkgdesc='This is a tool that demonstrates dumping MD5 password hashes from the configuration file in Skype.'
arch=('any')
url='http://packetstormsecurity.com/files/119155/Skype-Hash-Dumper-1.0.html'
license=('custom:unknown')
makedepends=('unzip')
source=('https://dl.packetstormsecurity.net/Win/skype_dump.zip')
sha512sums=('35c8f7734f4548d8902dde03499daa276b270e918952b08dfa1779826011e52eba75de4e3143cc283f9fbaf0e77910f859136f4e4bd78ba218a1f4bd8a74412b')

package() {
  install -dm 755 "$pkgdir/usr/share/windows/$pkgname"

  rm skype_dump.zip

  cp -a * "$pkgdir/usr/share/windows/$pkgname"
}

