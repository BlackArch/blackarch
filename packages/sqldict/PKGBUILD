pkgname=sqldict
pkgver=2.1
pkgrel=3
pkgdesc='A dictionary attack tool for SQL Server.'
url='http://ntsecurity.nu/toolbox/sqldict/'
groups=('blackarch' 'blackarch-windows' 'blackarch-webapp')
license=('custom:unknown')
arch=('any')
source=("https://dl.packetstormsecurity.net/Win/$pkgname.exe")
sha512sums=('27ed8ede7b908fbc8e5acdce4e73d025b341bfe64d982ec057c10b29ae840fc3b2686ebfe71c961f78b3cf716d1029e8756e76bb75e7dc83dfd23c46a02755fa')

package() {
  install -Dm 644 "$pkgname.exe" \
    "$pkgdir/usr/share/windows/$pkgname/$pkgname.exe"
}

