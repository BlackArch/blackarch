pkgname=python-azure-mgmt-web
pkgver=0.41.0
pkgrel=1
pkgdesc='Microsoft Azure Web Apps Management Client Library for Python.'
arch=('any')
url='https://pypi.org/project/azure-mgmt-web/#files'
license=('MIT')
depends=('python')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/82/1d/0a2992e9c00d6b8a88fd7e21e35a4caef086172e3bdcea0abcf990db9dfc/azure-mgmt-web-0.41.0.zip")
sha512sums=('b28993095e3387e9b9667c0ac89933546223fe3dd1ed8c3f4df14e3c866115aca24ddcdba48a3c5383fd2bc93ca0cfa6e30d1e905c62c45f1c9793090b687b99')

build() {
  cd "azure-mgmt-web-$pkgver"

  python setup.py build
}

package() {
  cd "azure-mgmt-web-$pkgver"

  python setup.py install --prefix=/usr --root="$pkgdir" --optimize=1 \
    --skip-build
}

