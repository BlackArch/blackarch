post_install() {
    echo ">"
    echo ">     Additionally, you can add your custom mimikatz executables (mimikatz.exe and mimikatz_x86.exe)"
    echo ">     in the MultiRelay/bin/ folder for the mimi32/mimi command."
    echo ">     A package is available in the repository."
    echo ">"
}

post_upgrade() {
    post_install "$@"
}
