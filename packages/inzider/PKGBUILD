pkgname=inzider
pkgver=1.2
pkgrel=2
pkgdesc='This is a tool that lists processes in your Windows system and the ports each one listen on.'
url='https://vidstromlabs.com/freetools/inzider/'
groups=('blackarch' 'blackarch-windows' 'blackarch-recon')
license=('custom:unknown')
arch=('any')
source=("https://vidstromlabs.com/downloads/$pkgname.exe")
sha512sums=('fea5ddf25cf8d53f740f7dba75f1a3139a5bf896eb8f79e4e69b1097dd7cb22e038463a7a90a32e00924ffb541309afb3cefdf62916f8a5459cd0ea745be6e6e')

package() {
  install -Dm 644 "$pkgname.exe" \
    "$pkgdir/usr/share/windows/$pkgname/$pkgname.exe"
}

