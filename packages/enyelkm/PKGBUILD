groups=('blackarch' 'blackarch-backdoor')
license=('GPL')
arch=('any')
source=("http://www.enye-sec.org/programas/$pkgname-$pkgver.tar.gz")
sha512sums=('1aafb17c9a76f01b513f05f609d25cd05f0854f3840c89174ae8ab57260f77848ceffde6d08389cbb16e7d0b63164e2111dd5a2beb5524415b64d90dc06ba12c')

package() {
  install -dm 755 "$pkgdir/usr/share"

  cp -a "$pkgname-$pkgver" "$pkgdir/usr/share/"
