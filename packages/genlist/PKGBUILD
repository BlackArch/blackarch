pkgname=genlist
pkgver=0.1
pkgrel=3
epoch=1
pkgdesc='Generates lists of IP addresses.'
groups=('blackarch' 'blackarch-misc')
license=('GPL')
arch=('x86_64' 'aarch64')
source=("http://dl.packetstormsecurity.net/UNIX/scanners/genlist.c")
sha512sums=('4d5c37af823926b45b197a046f6e390f1923a2c80716d2c7b15c889be471bc4194a4181958b3cf3cbdbe0a0134c9f17c5a9936abced4d32736e2bcd1fa908be1')

build() {
  gcc $CFLAGS "$pkgname.c" -o $pkgname
}

package() {
  install -Dm 755 $pkgname "$pkgdir/usr/bin/$pkgname"
}

