pkgname=pmdump
pkgver=1.2
pkgrel=3
pkgdesc='A tool that lets you dump the memory contents of a process to a file without stopping the process.'
url='http://ntsecurity.nu/toolbox/pmdump/'
groups=('blackarch' 'blackarch-windows' 'blackarch-forensic')
license=('custom:unknown')
arch=('any')
source=('http://ntsecurity.nu/downloads/pmdump.exe')
sha512sums=('0897945c9ae7e5bd91eb67ab142b1f47764ec5d4')

package() {
  cd "$srcdir"

  install -Dm644 pmdump.exe \
    "$pkgdir/opt/windows/archive/pmdump/pmdump.exe"
}
