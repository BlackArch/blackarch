pkgname=pmdump
pkgver=1.2
pkgrel=2
pkgdesc='A tool that lets you dump the memory contents of a process to a file without stopping the process.'
url='https://vidstromlabs.com/freetools/pmdump/'
groups=('blackarch' 'blackarch-windows' 'blackarch-forensic')
license=('custom:unknown')
arch=('any')
source=("https://vidstromlabs.com/downloads/$pkgname.exe")
sha512sums=('e76c85bac18fe01e8e1edf00a0b1dc7ffb3caf12ca66648ee4ab3ce5375aa1a04fc188ed6c4a69066c5570ae07a69be9d1b136ef7a18fc663828b438bf6de4e7')

package() {
  install -Dm 644 "$pkgname.exe" \
    "$pkgdir/usr/share/windows/$pkgname/$pkgname.exe"
}

