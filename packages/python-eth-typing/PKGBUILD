pkgname=python-eth-typing
_pkgname=eth-typing
pkgver=3.4.0
pkgrel=1
pkgdesc='Common type annotations for ethereum python packages.'
url='https://pypi.org/project/eth-typing/#files'
arch=('any')
license=('MIT')
depends=('python')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/source/${_pkgname::1}/$_pkgname/$_pkgname-$pkgver.tar.gz")
sha512sums=('963fa171a69904c0f5353c21e791953c75154f1543bbf28a1ac1a3e169471d047efad317a5ce0fd1db9f78e8ac0349856b15d5e1462cb7d32508ba7c2b4ea0f8')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

