pkgname=chromedriver
pkgver=2.41
pkgrel=2
pkgdesc="Standalone server which implements WebDriver's wire protocol."
arch=('x86_64' 'aarch64')
url='https://sites.google.com/a/chromium.org/chromedriver/'
license=('Apache')
depends=('libpng' 'gconf')
source=("http://chromedriver.storage.googleapis.com/$pkgver/${pkgname}_linux64.zip")
sha512sums=('328bd82d17f5d4238d3fd18b8848f4edc218014cba515972b75f1e4e866f1d729f6d56463b276f7afaf7965f0d36680c0238ae8ff8f9667bc6631e49ae9e7013')

package() {
  install -Dm 755 "$srcdir/$pkgname" "$pkgdir/usr/bin/$pkgname"
}

