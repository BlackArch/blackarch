pkgname=udptunnel
pkgver=19
pkgrel=4
epoch=2
groups=('blackarch' 'blackarch-networking' 'blackarch-tunnel')
pkgdesc='Tunnels TCP over UDP packets.'
arch=('x86_64' 'aarch64')
url='http://code.google.com/p/udptunnel/'
license=('GPL3')
source=("https://storage.googleapis.com/google-code-archive-downloads/v2/code.google.com/$pkgname/$pkgname-r${pkgver}.tar.gz")
#source=("https://udptunnel.googlecode.com/files/$pkgname-r$pkgver.tar.gz")
sha512sums=('0bd925887527d32cc0fcdd26d18e8f3603cfb22b45dc8360e5f91eb0eb3fce3fc51eb25d176d30fff9822614b1918f7671a33e4d0aac8a8e7e29d2b9336ad54c')

build() {
  cd $pkgname

  make
}

package() {
  cd $pkgname

  install -Dm 755 udptunnel "$pkgdir/usr/bin/$pkgname"
}

