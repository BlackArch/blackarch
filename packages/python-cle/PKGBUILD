pkgname=python-cle
_pkgname=cle
pkgver=9.0.10730
pkgrel=1
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python' 'python-pefile' 'python-cffi' 'python-pyvex'
         'python-minidump' 'python-sortedcontainers' 'python-pyvex'
         'python-pyelftools')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/08/e1/95b54f923881f78c5d174c6e3cf67eddb9b016f673fa23954810d97471a6/$_pkgname-$pkgver.tar.gz")
sha512sums=('a76785d36604f3dcd0f3601c99261409afca0c781e8ac73751543cf89a2687c05aa23d3f6691e28bf3391385e212d9a371df804889c88ed5b3e419d8c278c9c2')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

