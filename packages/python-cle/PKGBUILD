pkgname=python-cle
_pkgname=cle
pkgver=9.0.10576
pkgrel=2
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python' 'python-pefile' 'python-cffi' 'python-pyvex'
         'python-minidump' 'python-sortedcontainers' 'python-pyvex'
         'python-pyelftools')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/db/14/1b60bf66b755d8d1f240d6650ecc3b215755bcfd2bd25f7c2bce94fb8d95/$_pkgname-$pkgver.tar.gz")
sha512sums=('44dd7c70e47e59a0de3d9e3419f0fd6079264805c15f5a9cd4e6433866d4327fb122fb5b5cf2f98ec3c3d23cf5fe0c262970e8c867404f52e2ba11650e766784')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

