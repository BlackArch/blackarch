pkgname=python-cle
_pkgname=cle
pkgver=9.0.6488
pkgrel=1
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python' 'python-pefile' 'python-cffi' 'python-pyvex'
         'python-minidump' 'python-sortedcontainers' 'python-pyvex'
         'python-pyelftools')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/e1/55/5f01cb5105f1349a80af731ce8a48b275f1b4790c6d8fbea7554cce357b8/$_pkgname-$pkgver.tar.gz")
sha512sums=('6d26ccbef59288cfc0c35f61f7812943cd44e127574782eecf5f34fba47b6fb860f70174b47783aae5782eeec85be3ed481074cf7108b824ef65bdf83be5bbe4')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

