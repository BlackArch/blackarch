pkgname=python-cle
_pkgname=cle
pkgver=9.0.9947
pkgrel=1
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python' 'python-pefile' 'python-cffi' 'python-pyvex'
         'python-minidump' 'python-sortedcontainers' 'python-pyvex'
         'python-pyelftools')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/92/ee/1e73531bbc9291f0989cb58b62d08e9e45d19d8d846f59ea97b033904b29/$_pkgname-$pkgver.tar.gz")
sha512sums=('a9a22a6f5726e613afc5616a88d57b58d2dd42fd9bd0d199879667308c35dc1109922ecbf643a095cbba46825e8e9535caa5d431825525c974c3e750cd32ea31')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

