pkgname=python-cle
_name=cle
pkgver=8.18.10.25
pkgrel=1
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/98/89/672f0271144c98f38daed50f6ed8137f7072c571e3e8fb0e5d99e2898113/$_name-$pkgver.tar.gz")
sha512sums=('716a0de33ff3e91bd69a810e20ade76e0c256ce6a3acb07fcc4ae2047e6e9276603f4ad3bf886c5adc4edbaa7da3795bfdc3540d1d03056077b22eae83bfe578')

build() {
  cd "$_name-$pkgver"

  python setup.py build
}

package() {
  cd "$_name-$pkgver"

  python setup.py install --prefix=/usr --root="$pkgdir" --optimize=1 --skip-build
}

