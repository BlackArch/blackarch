pkgname=python-cle
_pkgname=cle
pkgver=9.2.14
pkgrel=1
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python' 'python-pefile' 'python-cffi' 'python-pyvex'
         'python-minidump' 'python-sortedcontainers' 'python-pyvex'
         'python-pyelftools')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/4b/8d/4aa04aeeb6f1005dd95c5930b7cd973b26a0fb03b9acb052f39de945963d/$_pkgname-$pkgver.tar.gz")
sha512sums=('41216a1f7904357b387f45e9b73208cbd9ee2eb416246f28a4bd5344896fcc82f6db05ac1a5fae19bb8a205250ee5914f2b8b92818415b28faebc06e60e59d0f')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

