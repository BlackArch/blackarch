pkgname=python-cle
_pkgname=cle
pkgver=9.0.6136
pkgrel=1
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python' 'python-pefile' 'python-cffi' 'python-pyvex'
         'python-minidump' 'python-sortedcontainers' 'python-pyvex'
         'python-pyelftools')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/f4/8a/08ba4f036260408051368fe742de62d56e9a40a52cca0e2167f93f3c390c/$_pkgname-$pkgver.tar.gz")
sha512sums=('97eac430c3edb828832eb61435bd7d0b57e4bfbc6a9b4cc12c6e77cf1e39d74380da5dc7670950b91ef76c468f778644d5f1418aaadd9d227f90642d0319c507')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

