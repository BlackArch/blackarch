pkgname=python-cle
_pkgname=cle
pkgver=9.0.6588
pkgrel=1
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python' 'python-pefile' 'python-cffi' 'python-pyvex'
         'python-minidump' 'python-sortedcontainers' 'python-pyvex'
         'python-pyelftools')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/f0/ed/1a8bd29b9f1ffc7a78a63d1e98fa81e54b3565dae0d8a23712528499e4f1/$_pkgname-$pkgver.tar.gz")
sha512sums=('54479296e988328646d5182b4e2a1ffe09487ba221667df31153d1fb7fc274d3b50e9e0a6aae4f6cf71b898bbd2e1ebd6b19a4c2cbdeeb8b5d5564f401065e5c')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

