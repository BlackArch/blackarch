pkgname=python-cle
_pkgname=cle
pkgver=9.0.10339
pkgrel=1
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python' 'python-pefile' 'python-cffi' 'python-pyvex'
         'python-minidump' 'python-sortedcontainers' 'python-pyvex'
         'python-pyelftools')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/c5/b5/da7c6ee43bc8e3e4d92f4e43cb73fa2b2694bef9c7d935707df4177ec629/$_pkgname-$pkgver.tar.gz")
sha512sums=('62bc72ef0094be8eb410bc0eaee29ec309807c09227f9b2e621fa27dfb2409486174d9dccbe90152c450111b889c365e18c308f18ab3e1a226b3e7b2d4bf8644')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

