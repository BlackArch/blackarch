pkgname=python-cle
_pkgname=cle
pkgver=9.0.5034
pkgrel=1
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python' 'python-pefile' 'python-cffi' 'python-pyvex'
         'python-minidump' 'python-sortedcontainers' 'python-pyvex'
         'python-pyelftools')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/47/a9/a3252f75a6d6e06735086dfff75967a72cc457e12f03758ad732735ba580/$_pkgname-$pkgver.tar.gz")
sha512sums=('6ecb24aef042ce75c3a649fdeaf98ffb9115dbb1047f7a5e52c9d4342a06b2c03eb1292360ef4416e7bea114b2847e402c401654000efd95d28e695beaec1422')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

