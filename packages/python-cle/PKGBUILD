pkgname=python-cle
_pkgname=cle
pkgver=9.0.6281
pkgrel=1
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python' 'python-pefile' 'python-cffi' 'python-pyvex'
         'python-minidump' 'python-sortedcontainers' 'python-pyvex'
         'python-pyelftools')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/cb/52/3e5faf67f05bc7e589c157bb6638fe9994db8e6bfa42aab62c298617f9b8/$_pkgname-$pkgver.tar.gz")
sha512sums=('568bd041c0bec00f5f268f780d0842c7ef74285507851570dbc71a26a2b12ce22d4a486c3f66aa016cd7c1d3c300c98024b37dd9a3b3846b92a2e3a01701c1cb')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

