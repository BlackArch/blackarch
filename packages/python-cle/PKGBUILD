pkgname=python-cle
_pkgname=cle
pkgver=9.0.5903
pkgrel=1
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python' 'python-pefile' 'python-cffi' 'python-pyvex'
         'python-minidump' 'python-sortedcontainers' 'python-pyvex'
         'python-pyelftools')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/1c/02/4400b4686fb4164005de8c24c43f4d5b4053bba5c6cf46d413a09cc21063/$_pkgname-$pkgver.tar.gz")
sha512sums=('2cb9402cb13d26db33b78c48f5151cda5509d8ca0fbe3943759c9a97a49e23c905d2f73288d44f4fb718f7d020c3db5ba49aa8be2fd1baeb959dbf47350f1957')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

