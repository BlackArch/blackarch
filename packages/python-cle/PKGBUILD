pkgname=python-cle
_pkgname=cle
pkgver=9.0.10159
pkgrel=1
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python' 'python-pefile' 'python-cffi' 'python-pyvex'
         'python-minidump' 'python-sortedcontainers' 'python-pyvex'
         'python-pyelftools')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/fa/28/43cff2b01f604f173bfcbcec6ab4e8534765922e0ebfecb77b39ee13b16c/$_pkgname-$pkgver.tar.gz")
sha512sums=('18fa8c9b60ff5cd469de7f8a353f6968a85282e3eab9d2e5ab48f6f1ec43040b2f8c2f3e26156da418c8b463d50065da3c020f1c06a13554fbd4b90287fed112')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

