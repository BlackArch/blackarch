pkgname=python-cle
_pkgname=cle
pkgver=9.0.9506
pkgrel=1
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python' 'python-pefile' 'python-cffi' 'python-pyvex'
         'python-minidump' 'python-sortedcontainers' 'python-pyvex'
         'python-pyelftools')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/d6/94/4e70bb74f087c847280ea52ee33d70cfb32645b5a434aec85ae9ddef292e/$_pkgname-$pkgver.tar.gz")
sha512sums=('a4760f7f685f7cedad4d31d74fcf66ecc300895705d339fac33b25314df68f53b7fa43e749715af02d0ed8609c9597817b4ed097ce01d090a135b731ef311d54')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

