pkgname=python-cle
_pkgname=cle
pkgver=9.0.9684
pkgrel=1
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python' 'python-pefile' 'python-cffi' 'python-pyvex'
         'python-minidump' 'python-sortedcontainers' 'python-pyvex'
         'python-pyelftools')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/9c/a7/810d0ee64601245a0036ea0e31f05d512677699c4b5973840a1974e9eebd/$_pkgname-$pkgver.tar.gz")
sha512sums=('aaadde67dfd5d8c91045dd25c057e1417b964a540dc3da1d97e7016024b25c2d6574ab0e748fba587aa7bef3c094d39fc1813f5db8390e4f7f5379820b0f5149')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

