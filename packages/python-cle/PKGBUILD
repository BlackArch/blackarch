pkgname=python-cle
_pkgname=cle
pkgver=9.0.6852
pkgrel=1
pkgdesc='Interface to analyze what they are and what they would look like in memory.'
arch=('any')
url='https://pypi.org/project/cle/#files'
license=('BSD')
depends=('python' 'python-pefile' 'python-cffi' 'python-pyvex'
         'python-minidump' 'python-sortedcontainers' 'python-pyvex'
         'python-pyelftools')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/91/57/8ab1981754cbe3c94146f336600e78b67c4e7df1300a12c1f44e6c076fc3/$_pkgname-$pkgver.tar.gz")
sha512sums=('d9b2de7bfc8b43653449c90263c1e5e9d640f2dd9b40f2c081ccfa982ae47445467de6355994d6f3f81f6e47e65b8c51d61eb065b064c0a65b035f2140682617')

build() {
  cd "$_pkgname-$pkgver"

  python setup.py build
}

package() {
  cd "$_pkgname-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr -O1 --skip-build
}

