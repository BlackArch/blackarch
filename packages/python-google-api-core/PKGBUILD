pkgname=python-google-api-core
pkgver=1.11.0
pkgrel=1
pkgdesc='Google API client core library.'
arch=('any')
url='https://pypi.org/project/google-api-core/#files'
license=('Apache')
depends=('python' 'python-googleapis-common-protos')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/d3/82/e85699e1ee442aa936b0ec0081fd032ffda7df34c7df8b148bca94c1ae00/google-api-core-1.11.0.tar.gz")
sha512sums=('cbf2ba65c60cf1fab8bf7343f8ab71654778407fae3f38994709f48f8a58d590a8ffaede0714b3842880215888fe8a200e5467e0b9c99a9d747428936a113e21')

build() {
  cd "google-api-core-$pkgver"

  python setup.py build
}

package() {
  cd "google-api-core-$pkgver"

  python setup.py install --root="$pkgdir" --prefix=/usr --optimize=1 \
    --skip-build
}

