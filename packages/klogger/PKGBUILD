pkgname=klogger
pkgver=1.0
pkgrel=6
pkgdesc='A keystroke logger for the NT-series of Windows.'
url='http://ntsecurity.nu/toolbox/klogger/'
groups=('blackarch' 'blackarch-windows' 'blackarch-keylogger')
license=('custom')
arch=('any')
source=("https://web.archive.org/web/20120630171751/http://ntsecurity.nu/downloads/$pkgname.exe")
sha512sums=('2e4f78bf876820ac504526a71e66745b6c4b5eb92f6a963bd5cdabfe39300ca1652c2d7eb8410812197bcdabb78ace01fc88c1bf042df391f7fac5e28f7677e5')

package() {
  install -Dm 644 "$pkgname.exe" \
    "$pkgdir/usr/share/windows/$pkgname/$pkgname.exe"
}

