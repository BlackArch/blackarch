pkgname=python-mulpyplexer
_name=mulpyplexer
pkgver=0.08
pkgrel=1
pkgdesc='Multiplex interactions with lists of python objects.'
arch=('any')
url='https://pypi.org/project/mulpyplexer/#files'
license=('BSD')
depends=('python')
makedepends=('python-setuptools')
source=("https://files.pythonhosted.org/packages/37/98/d9313a5f3d491729b7902ac70783022f08150e6036e7d1e565ea342db968/$_name-$pkgver.tar.gz")
sha512sums=('6a8e3aaeaa42a1ec90bbd34aefa6c9c259e8b3e8a01e0824fce9cf4e82ae65aedec747eb1849ef7341bab144be739ae86378fdd825d4eefa3bbf89ca96a87ab3')

build() {
  cd "$_name-$pkgver"

  python setup.py build
}

package() {
  cd "$_name-$pkgver"

  python setup.py install --prefix=/usr --root="$pkgdir" --optimize=1 --skip-build
}

