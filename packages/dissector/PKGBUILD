pkgname=dissector
pkgver=1
pkgrel=3
groups=('blackarch' 'blackarch-binary')
pkgdesc='This code dissects the internal data structures in ELF files. It supports x86 and x86_64 archs and runs under Linux.'
arch=('x86_64')
url='http://packetstormsecurity.com/files/125972/Coloured-ELF-File-Dissector.html'
license=('custom:unknown')
source=("http://dl.packetstormsecurity.net/UNIX/utilities/dissector.c")
sha512sums=('3f70f1e299155939c4ab96095fdcb3c4e71a2d0019c7ddce43a3318b05a9967f798ca4f3e42eece2c17e6e21a794521c01db1a65217b5470c28b889c1fa9bf96')

build() {
  gcc -Wall "$pkgname.c" -o $pkgname
}

package() {
  install -Dm 755 $pkgname "$pkgdir/usr/bin/$pkgname"
}

