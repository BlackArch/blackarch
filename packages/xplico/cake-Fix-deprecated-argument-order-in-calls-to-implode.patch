From: Jeremy Ruten <jeremy.ruten@gmail.com>
Date: Wed, 17 Jun 2020 11:44:23 -0600
Subject: [PATCH] Fix deprecated argument order in calls to implode()

Origin: upstream, https://github.com/cakephp/cakephp/commit/e90e4716ec4eff9518ba36e228341a1b554e2be6;
---
 system/xi3/lib/Cake/Console/Command/CompletionShell.php | 2 +-
 system/xi3/lib/Cake/View/Helper/FormHelper.php          | 4 ++--
 2 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/system/xi3/lib/Cake/Console/Command/CompletionShell.php b/system/xi3/lib/Cake/Console/Command/CompletionShell.php
index 3d6fa17..21ee0e7 100644
--- a/system/xi3/lib/Cake/Console/Command/CompletionShell.php
+++ b/system/xi3/lib/Cake/Console/Command/CompletionShell.php
@@ -149,7 +149,7 @@ class CompletionShell extends AppShell {
  */
 	protected function _output($options = array()) {
 		if ($options) {
-			return $this->out(implode($options, ' '));
+			return $this->out(implode(' ', $options));
 		}
 	}
 }
diff --git a/system/xi3/lib/Cake/View/Helper/FormHelper.php b/system/xi3/lib/Cake/View/Helper/FormHelper.php
index eabea8d..0081b77 100644
--- a/system/xi3/lib/Cake/View/Helper/FormHelper.php
+++ b/system/xi3/lib/Cake/View/Helper/FormHelper.php
@@ -609,8 +609,8 @@ class FormHelper extends AppHelper {
 		ksort($locked, SORT_STRING);
 		$fields += $locked;
 
-		$locked = implode(array_keys($locked), '|');
-		$unlocked = implode($unlockedFields, '|');
+		$locked = implode('|', array_keys($locked));
+		$unlocked = implode('|', $unlockedFields);
 		$hashParts = array(
 			$this->_lastAction,
 			serialize($fields),
