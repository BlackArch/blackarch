arch=('any')
license=('custom:unknown')
depends=('perl')
source=("$pkgname::http://magikh0e.xyz/pubCode/fprotLogParser.txt")
sha512sums=('bd69ded079829bb515aa0507fde1b649d033280b')

package() {
  install -Dm 755 $pkgname "$pkgdir/usr/bin/$pkgname"
}

