pkgname=cachedump
pkgver=1.1
pkgrel=4
pkgdesc='A tool that demonstrates how to recover cache entry information: username and hashed password (called MSCASH).'
url='https://packetstormsecurity.com/files/36781/cachedump-1.1.zip.html'
groups=('blackarch' 'blackarch-windows' 'blackarch-cracker')
license=('custom:unknown')
arch=('any')
source=("https://dl.packetstormsecurity.net/Win/$pkgname-$pkgver.zip")
sha512sums=('a901423136280c978d4fabb458c3e6991ae6276cebdea06afc1c37e1603ff4ac38855186c5ecf6baebcd72855a4626210806f4d16eb3ec7ff89b5961177afe30')

package() {
  install -dm 755 "$pkgdir/usr/share/windows/$pkgname"

  rm "$pkgname-$pkgver.zip"

  cp -a * "$pkgdir/usr/share/windows/$pkgname"
}

