pkgname=winfo
pkgver=2.0
pkgrel=6
pkgdesc='Uses null sessions to remotely try to retrieve lists of and information about user accounts, workstation/interdomain/server trust accounts, shares (also hidden), sessions, logged in users, and password/lockout policy, from Windows NT/2000/XP.'
url='https://vidstromlabs.com/freetools/winfo/'
groups=('blackarch' 'blackarch-windows' 'blackarch-recon' 'blackarch-scanner')
license=('custom')
arch=('any')
source=("https://vidstromlabs.com/downloads/$pkgname.exe")
sha512sums=('b5237222250f55d83163fe82d0e31f3e4c0001a5b2d891d3666a5c9563156238d6916e15ff8da7c8edb69659dfba3a52aa41afe337127066e1ff670796c89959')

package() {
  install -Dm 644 "$pkgname.exe" \
    "$pkgdir/usr/share/windows/$pkgname/$pkgname.exe"
}

