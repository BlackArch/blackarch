sha512sums=('f29cb7cdd17b09d8a38bdd469a389e0b42093eaf34c44645f67c05ac6cf1c8ad685ef58158c58064112a3b08b626bc25fb7e598f0b8a34b1cd4aeca1a8a738e9')

package() {
  install -Dm 755 floss "$pkgdir/usr/bin/$pkgname"
}

