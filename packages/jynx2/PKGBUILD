pkgname=jynx2
pkgver=2.0
pkgrel=1
pkgdesc="An expansion of the original Jynx LD_PRELOAD rootkit"
arch=('any')
url="http://www.blackhatlibrary.net/Jynx2"
license=('GPL') # this may be wrong. no license information is provided anywhere
groups=('blackarch-exploitation')
install=jynx2.install
source=("http://blackhatlibrary.net/releases/$pkgname.tgz")
sha256sums=('8aed104a95e0968ecd5e1edac63a89615a69f27a46f562a20f107543a6ce2099')

package() {
	cd $pkgname

	#make DESTDIR="$pkgdir/" install
  mkdir -pm 755 "$pkgdir"/usr/share/$pkgname
  cp -r * "$pkgdir"/usr/share/$pkgname
}
