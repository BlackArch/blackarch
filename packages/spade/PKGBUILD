pkgname=spade
pkgver=114
pkgrel=6
pkgdesc='A general-purpose Internet utility package, with some extra features to help in tracing the source of spam and other forms of Internet harassment.'
url='http://www.hoobie.net/brutus/'
groups=('blackarch' 'blackarch-windows' 'blackarch-scanner' 'blackarch-recon')
license=('custom:unknown')
arch=('any')
source=("http://examples.oreilly.com/networksa/tools/${pkgname}${pkgver}.exe")
sha512sums=('f6b72a9933c234915d68053b511e6c8977eae1a7f7ed8ae347964948c04f56f2e52ef440e089dc04766a49d28032be3dfb3b995d87dc4158c9887f24afd4e04c')

package() {
  install -Dm 644 "${pkgname}${pkgver}.exe" \
    "$pkgdir/usr/share/windows/$pkgname/$pkgname.exe"
}

