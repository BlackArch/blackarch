pkgname='v3n0m'
pkgver=4.0.1
pkgrel=1
groups=('blackarch')
pkgdesc='A tool to automate mass SQLi d0rk scans and Metasploit Vulns'
arch=('any')
url='https://github.com/v3n0m-Scanner/V3n0M-Scanner/tree/master'
license=('GPL')
depends=('python3')
makedepends=('git')
source=('git+https://github.com/v3n0m-Scanner/V3n0M-Scanner.git')
sha1sums=('SKIP')

package() {
  cd "$srcdir/V3n0M-Scanner"
  python3 setup.py install --prefix=/usr --root="$pkgdir"
}
